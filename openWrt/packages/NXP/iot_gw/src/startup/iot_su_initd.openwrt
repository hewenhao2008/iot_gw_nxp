START=99

PROG=iot_su.sh

start () {
    echo Starting /usr/bin/$PROG
    if [ -z `pidof $PROG` ]; then
        /usr/bin/$PROG > /dev/null &
    fi
}

stop () {
    echo Will not stop $PROG, must do by hand
}