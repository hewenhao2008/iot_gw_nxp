CSS  = iot.css slider.css c3.css mobile.css

all: clean build

build:
	mkdir -p ../../swupdate/images/www/css
	cp -f $(CSS) ../../swupdate/images/www/css

clean:
	-rm -Rf ../../swupdate/images/www/css

